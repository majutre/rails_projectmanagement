<div class="card">
  <div class="card-body">

    <h2 class="card-title d-flex justify-content-between mb-4">
      <span>Projects</span>
      <%= link_to 'New Project', new_project_path, class:'btn btn-primary' %>
    </h2>
    <hr class="mt-2 mb-4" />

    <%# render(ProjectOverviewComponent.with_collection(@projects)) %>
   
    <% @projects.each do |project| %>
      <%= render(ProjectOverviewComponent.new(project: project)) %>  
    <% end %>

  </div>  
</div>


